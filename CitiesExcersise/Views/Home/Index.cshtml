@model IEnumerable<City>

@{ Layout = "_Layout";}

<table class="table table-sm table-bordered">
    <thead class="bg-primary text-white">
        <tr>
            <td>Miasto</td>
            <td>Kraj</td>
            <td class="text-right">Populacja</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var c in Model)       //zamienilem var na City
        {
            <tr>
                <td>@c.Name</td>
                <td>@c.Country</td>
                <td class="text-right">@c.Population?.ToString("#,###")</td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Create" class="btn btn-primary">Dodaj miasto</a>