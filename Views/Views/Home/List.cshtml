@{ Layout = null; }
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Razor</title>
    <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.css" />
    <script id="jsonData" type="application/json">
        @Json.Serialize(new string[] { "Jabłka", "Pomarańcze", "Gruszki"})
    </script>
    <script src="~/lib/jquery/jquery.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var list = $("#list")
            JSON.parse($("#jsonData").text()).forEach(function (val) {
                console.log("Wartość: " + val);
                list.append($("<li>").text(val));
            });
        });
    </script>
</head>
<body class="m-1 p-1">
    To jest widok częściowy
    <ul id="list"></ul>
    
</body>
</html>